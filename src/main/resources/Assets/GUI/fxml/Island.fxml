<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>

<?import it.polimi.softeng.network.client.view.FXML_Controllers.*?>
<fx:root type="javafx.scene.layout.AnchorPane" minWidth="100" maxWidth="-Infinity" minHeight="100" maxHeight="-Infinity" xmlns:fx="http://javafx.com/fxml">
    <children>
        <ImageView fx:id="islandImage"/>
        <ButtonWithLabel fx:id="motherNature"/>
        <ButtonWithLabel fx:id="yellow" button-style="-fx-background-image: url('/Assets/GUI/Icons/Yellow_Disk.png');-fx-background-size: contain" />
        <ButtonWithLabel fx:id="blue" button-style="-fx-background-image: url('/Assets/GUI/Icons/Blue_Disk.png');-fx-background-size: contain" />
        <ButtonWithLabel fx:id="green" button-style="-fx-background-image: url('/Assets/GUI/Icons/Green_Disk.png');-fx-background-size: contain" />
        <ButtonWithLabel fx:id="red" button-style="-fx-background-image: url('/Assets/GUI/Icons/Red_Disk.png');-fx-background-size: contain" />
        <ButtonWithLabel fx:id="purple" button-style="-fx-background-image: url('/Assets/GUI/Icons/Purple_Disk.png');-fx-background-size: contain" />
    </children>
</fx:root>
